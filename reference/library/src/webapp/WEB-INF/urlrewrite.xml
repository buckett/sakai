<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

    <rule>
        <from>^/js/editor/ckeditor/(.*)$</from>
        <to>/webjars/ckeditor/4.4.7/$1</to>
    </rule>
    <rule>
        <from>^/js/bootstrap/(.*)$</from>
        <to>/webjars/bootstrap/$1</to>
    </rule>
    <rule>
        <from>^/image/silk/(.*)$</from>
        <to>/webjars/famfamfam-silk/$1</to>
    </rule>
    <rule>
        <from>^/js/moment/(.*)$</from>
        <to>/webjars/moment/$1</to>
    </rule>

</urlrewrite>
